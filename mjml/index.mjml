      <meta name="format-detection" content="email=no"/>
    </mj-raw>
    <mj-attributes>
      <mj-all font-family="Arial, Helvetica, sans-serif"/>
      <mj-class name="+2T" font-family="&quot;kari-display-pro&quot;, sans-serif" font-size="65px" line-height="69px" font-weight="400" font-style="normal"></mj-class>
      <mj-class name="+1T" font-family="&quot;Outfit&quot;, serif" font-style="normal"></mj-class>
      <mj-class name="0T" font-size="16px" line-height="20px" font-weight="400" font-style="normal"/>
